# Environment variables for Elysian project

# Set to 'true' to use SQLite for development, 'false' for PostgreSQL
USE_SQLITE=true

# Database connection string
# For SQLite (development)
DATABASE_URL=file:./elysian.sqlite

# For PostgreSQL (production) - uncomment and update when ready
# DATABASE_URL=postgresql://username:password@localhost:5432/elysian_db
